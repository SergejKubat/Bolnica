<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="500.0" stylesheets="file:css/pacijenti.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RezultatPregledaController">
