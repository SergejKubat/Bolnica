            <Font name="Ebrima" size="14.0" />
         </font>
      </Button>
      <TableView fx:id="dijagnozeTabela" layoutX="10.0" layoutY="10.0" onMouseClicked="#prikaziLekove" prefHeight="300.0" prefWidth="480.0">
