          <TableColumn fx:id="bolestColumn" prefWidth="110.0" text="Bolest" />
          <TableColumn fx:id="opisDijagnozeColumn" prefWidth="369.0" text="Opis dijagnoze" />
        </columns>
      </TableView>
      <Label fx:id="prepisaniLekoviText" layoutX="10.0" layoutY="350.0" text="Prepisani lek(ovi):" visible="false">
         <font>
            <Font name="Ebrima Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="prepisaniLekovi" layoutX="135.0" layoutY="350.0" text="text" visible="false">
         <font>
            <Font name="Ebrima" size="14.0" />
         </font>
      </Label>
